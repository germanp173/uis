{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "no-console": false,
        "ordered-imports": false,
        "jsx-boolean-value": false,
        "interface-name": false,
        "indent": false // Covered by Prettier
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}